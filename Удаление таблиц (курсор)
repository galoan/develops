
 DECLARE @CURSOR	CURSOR 
 SET	 @CURSOR  = CURSOR FOR  

 SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME NOT IN ('RPZ_zk_modInfo','MyTableName','Realize') --IN ('TEST_TBL','TEST_TBL2')

 OPEN @CURSOR  
 WHILE 1=1 
 BEGIN 
 DECLARE @TBL_NAME	SYSNAME
		,@sql	    NVARCHAR(4000)
 SET	 @TBL_NAME=NULL
 FETCH   @CURSOR INTO @TBL_NAME
 
 IF @@FETCH_STATUS <> 0 BREAK 
 
 SELECT @sql = 
			'drop table [1C_Scribe].[dbo].['
			+ @TBL_NAME
			+ ']'
		--PRINT @sql
		EXEC(@sql)

 END
 CLOSE @CURSOR   
 DEALLOCATE @CURSOR  

 GO
